{
  "compilerOptions": {
    "strictNullChecks": false,
    "strict": true,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "esModuleInterop": true,
    "jsx": "preserve",
    "lib": ["es2015", "es2016", "es2017", "es2018", "es2019", "es2020", "dom"],
    "target": "es5",
    "baseUrl": ".",
    "outDir": "."
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
